// Place your key bindings in this file to override the defaults
[
    {
        "key": "alt+f",
        "command": "search.action.openNewEditor"
    },
    {
        "command": "deleteWordRight",
        "key": "alt+delete",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "command": "deleteWordLeft",
        "key": "alt+backspace",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "command": "cursorHome",
        "key": "alt+left",
        "when": "textInputFocus"
    },
    {
        "command": "cursorEnd",
        "key": "alt+right",
        "when": "textInputFocus"
    }
]