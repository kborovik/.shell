{
  "SQL-Formatter-VSCode.dialect": "bigquery",
  "SQL-Formatter-VSCode.functionCase": "upper",
  "SQL-Formatter-VSCode.logicalOperatorNewline": "after",
  "[dockercompose]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[helm]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "cSpell.advanced.feature.useReferenceProviderRemove": "/^#+\\s/",
    "cSpell.advanced.feature.useReferenceProviderWithRename": true,
    "cSpell.fixSpellingWithRenameProvider": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one",
    "editor.quickSuggestions": {
      "comments": "off",
      "other": "off",
      "strings": "on"
    },
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.wordWrap": "on"
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.tabSize": 4
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "cSpell.enableFiletypes": [
    "ansible",
    "dockercompose",
    "helm",
    "makefile",
    "shellscript",
    "terraform"
  ],
  "cSpell.enabled": true,
  "cSpell.userWords": [
    "abspath",
    "atuin",
    "gcloud",
    "gsutil",
    "ifeq",
    "ifneq",
    "lastword",
    "northamerica",
    "patsubst",
    "tflock",
    "tfvars",
    "toset"
  ],
  "chat.editor.fontFamily": "IBM Plex Mono, Fira Code Retina, Meslo",
  "chat.editor.fontSize": 16,
  "cody.commandCodeLenses": false,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.colorDecorators": false,
  "editor.defaultFormatter": null,
  "editor.detectIndentation": true,
  "editor.fontFamily": "IBM Plex Mono, Fira Code Retina, Meslo",
  "editor.fontLigatures": true,
  "editor.fontSize": 17,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.showToolbar": "onHover",
  "editor.inlineSuggest.suppressSuggestions": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "all",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "One Dark italic",
        "scope": [
          "comment",
          "entity.other.attribute-name",
          "keyword",
          "markup.underline.link",
          "storage.modifier",
          "storage.type",
          "string.url",
          "variable.language.super",
          "variable.language.this"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "One Dark italic reset",
        "scope": [
          "keyword.operator",
          "keyword.other.type",
          "storage.modifier.import",
          "storage.modifier.package",
          "storage.type.built-in",
          "storage.type.function.arrow",
          "storage.type.generic",
          "storage.type.java",
          "storage.type.primitive"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "explorer.confirmDelete": false,
  "files.associations": {
    "**/templates/*.yaml": "helm",
    "**/templates/*.yml": "helm",
    "*.bqsql": "sql",
    "*.hcl": "terraform",
    "*.service": "toml",
    "*.tfvars": "terraform",
    "*.toml": "toml",
    "main.yaml": "ansible"
  },
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.confirmSync": false,
  "git.mergeEditor": true,
  "git.openRepositoryInParentFolders": "prompt",
  "html.format.extraLiners": "head, body, footer, /html",
  "html.format.maxPreserveNewLines": 1,
  "oneDark.italic": true,
  "prettier.bracketSpacing": false,
  "prettier.ignorePath": ".prettierignore",
  "prettier.insertPragma": false,
  "prettier.printWidth": 120,
  "python.defaultInterpreterPath": "/home/kb/python/it-vend-track/.venv",
  "redhat.telemetry.enabled": false,
  "ruff.fixAll": false,
  "ruff.showNotifications": "onWarning",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.defaultProfile.linux": "bash",
  "terminal.integrated.defaultProfile.osx": "bash",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "/usr/bin/bash"
    },
    "dash": {
      "path": "/usr/bin/dash"
    }
  },
  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "/opt/homebrew/bin/bash"
    }
  },
  "vs-kubernetes": {
    "vs-kubernetes.crd-code-completion": "disabled",
    "vs-kubernetes.ignore-recommendations": true,
    "vscode-kubernetes.helm-path-mac": "/Users/kborovik/.vs-kubernetes/tools/helm/darwin-arm64/helm",
    "vscode-kubernetes.kubectl-path-mac": "/Users/kborovik/.vs-kubernetes/tools/kubectl/kubectl",
    "vscode-kubernetes.minikube-path-mac": "/Users/kborovik/.vs-kubernetes/tools/minikube/darwin-arm64/minikube"
  },
  "window.zoomLevel": 2,
  "workbench.colorTheme": "One Dark",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "cody.chat.defaultLocation": "sidebar"
}